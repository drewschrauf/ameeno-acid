{
  "name": "ameeno-acid",
  "version": "0.0.1",
  "description": "A fully asynchronous static site generator using Marko templates",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf lib && babel src --out-dir lib",
    "test": "MARKO_CLEAN=true NODE_ENV=test mocha --compilers js:babel-register",
    "watch": "MARKO_CLEAN=true NODE_ENV=test mocha -w --compilers js:babel-register",
    "coverage": "rm -rf coverage && MARKO_CLEAN=true NODE_ENV=test babel-node ./node_modules/.bin/isparta cover _mocha -- --recursive",
    "lint": "eslint src",
    "xprepublish": "npm run lint && npm run test && npm run build"
  },
  "author": "",
  "license": "ISC",
  "peerDependencies": {
    "marko": "2.x"
  },
  "dependencies": {
    "extend": "^3.0.0",
    "markdown": "^0.5.0",
    "routes": "^2.1.0",
    "verror": "^1.6.0"
  },
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.17",
    "babel-eslint": "^5.0.0",
    "babel-plugin-rewire": "^1.0.0-beta-3",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.3.13",
    "chai": "^3.4.1",
    "chai-as-promised": "^5.1.0",
    "eslint": "^1.10.3",
    "eslint-config-defaults": "^7.1.1",
    "isparta": "^4.0.0",
    "mocha": "^2.1.0",
    "mock-fs": "^3.5.0",
    "rewire": "^2.3.4",
    "sinon": "^1.17.2",
    "sinon-chai": "^2.8.0"
  }
}
